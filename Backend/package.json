{
  "name": "backend",
  "version": "1.0.0",
  "description": "StayGenie backend server",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test:performance": "ts-node tests/testRunner.ts",
    "test:perf": "npm run test:performance",
    "test:perf:force": "ts-node tests/testRunner.ts --force",
    "test:perf:clean": "rm -f tests/performance-*.html tests/performance-*.json",
    "perf:install": "echo 'Performance testing dependencies already included'"
  },
  "keywords": [
    "hotel-search",
    "travel",
    "optimization",
    "performance",
    "ai-recommendations",
    "redis-cache",
    "performance-testing",
    "step-analytics",
    "bottleneck-detection"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/auth": "^22.4.0",
    "@supabase/supabase-js": "^2.50.0",
    "@xenova/transformers": "^2.17.2",
    "amadeus": "^11.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cohere-ai": "^7.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "groq-sdk": "^0.27.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "next": "^15.4.1",
    "openai": "^4.104.0",
    "p-limit": "^3.1.0",
    "redis": "^4.7.1",
    "undici": "^5.29.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/tests/**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/tests/performanceTest.ts",
      "/tests/testRunner.ts",
      "/tests/testPrompts.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!tests/performanceTest.ts",
      "!tests/testRunner.ts",
      "!tests/testPrompts.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
